<div class = 'print sixteen columns'>
	<label class='two columns'>Title:</label><label id='title' class='thirteen columns'> {{title}}</label>
	<label class='two columns'>Rank:</label><label id='rank' class='thirteen columns'> {{rank}}</label>
	<p id='description' class='fourteen columns'>{{{description}}}</p>
</div><div class='edit sixteen columns'><div class='fourteen columns'>
	<label class='two columns'>Title: </label><input id='title' type='text' value = '{{title}}'></input>
	<label class='two columns'>Rank: </label><input id='rank' type='text' value = '{{rank}}'></input>
	<label class='one columns'>Description: </label><textarea id='description' class='fourteen columns'>{{{description}}}</textarea><form class='noactionupdate' action='/rest/Module.class_module.new/' method='POST'><input type='hidden' value='{{id}}'></input><input type='submit' value='Add New Class to Class_module'></submit></form><form class='noactionupdate' action='/rest/Module.module_assignment.new/' method='POST'><input type='hidden' value='{{id}}'></input><input type='submit' value='Add New Assignment to Module_assignment'></submit></form>
</div><button class='__done__ one columns'>Done</button></div><div class='view sixteen columns'><div class='fourteen columns'>
	<label class='one columns'>Title:</label><p id='title' class='thirteen columns'> {{title}}</p>
	<label class='one columns'>Rank:</label><p id='rank' class='thirteen columns'> {{rank}}</p>
	<label class='one columns'>Description: </label><p id='description' class='sixteen columns'>{{{description}}}</p>
</div><button class='__edit__ one columns'>Edit</button></div>